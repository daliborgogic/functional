{
  "name": "functional",
  "version": "1.0.0",
  "description": "Functional JavaScript",
  "scripts": {
    "test": "ava -v",
    "lint": "eslint --ext .mjs --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.6",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "extensions": [
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "standard"
    ]
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "eslint-config-idiomatic": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daliborgogic/functional.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daliborgogic/functional/issues"
  },
  "homepage": "https://github.com/daliborgogic/functional#readme"
}
